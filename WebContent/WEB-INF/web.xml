<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>vaccination</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <context-param>
        <param-name>JDBC_DRIVER</param-name>
        <param-value>com.mysql.jdbc.Driver</param-value>
    </context-param>

    <context-param>
        <param-name>JDBC_URL</param-name>
        <param-value>jdbc:mysql://localhost/digitronik?useTimezone = true &amp; serverTimezone = UTC</param-value>
    </context-param>

    <context-param>
        <param-name>JDBC_LOGIN</param-name>
        <param-value>vaccination</param-value>
    </context-param>

    <context-param>
        <param-name>JDBC_PASSWORD</param-name>
        <param-value>vaccination</param-value>
    </context-param>
    
    <jsp-config>
		<taglib>
			<taglib-uri>http://vaccination.ma/tld/extratags</taglib-uri>
			<taglib-location>/WEB-INF/extratags.tld</taglib-location>
		</taglib>
	</jsp-config>
</web-app>